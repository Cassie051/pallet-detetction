<?xml version="1.0"?>

<launch>
  <arg name="pose_x" default="0"/>
  <arg name="pose_y" default="0"/>
  <arg name="pose_z" default="0"/>
  <arg name="pose_roll" default="0"/>
  <arg name="pose_pitch" default="0"/>
  <arg name="pose_yaw" default="0"/>
  
  <param name="name" command="$(find gazebo_models_spds)/models/dumpster/model.sdf" />
  
  <node name="spawn_dumpster_model" pkg="gazebo_ros" type="spawn_model"
        args="-unpause -sdf spawn_model -file $(arg name) -x $(arg pose_x) -y $(arg pose_y) -z $(arg pose_z) -R $(arg pose_roll) -P $(arg pose_pitch) -Y $(arg pose_yaw)" respawn="false" output="screen" />

</launch>
